<netmrg>
	<database>
		<host>localhost</host>
		<user>netmrguser</user>
		<password>netmrgpass</password>
		<db>netmrg</db>
	</database>
	<paths>
		<rrdtool>@RRDTOOL@</rrdtool>
		<lockfile>@prefix@/var/log/netmrg/lockfile</lockfile>
		<runtimefile>@prefix@/var/log/netmrg/runtime</runtimefile>
		<libexec>@exec_prefix@/libexec/netmrg</libexec>
		<rrds>@prefix@/var/lib/netmrg/rrd</rrds>
		<webfileroot>@prefix@/var/www/netmrg</webfileroot>
	</paths>
	<threads>
		<count>5</count>
		<sleep>10000</sleep>
	</threads>
	<website>
		<company>Generic Company</company>
		<companylink>http://www.netmrg.net</companylink>
		<webhost>http://localhost</webhost>
		<webroot>/netmrg</webroot>
	</website>
	<polling>
		<interval>300</interval>
	</polling>
</netmrg>
